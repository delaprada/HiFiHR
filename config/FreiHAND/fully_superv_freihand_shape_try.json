    "controlled_size": 2000,
    "train_batch":64,
    "val_batch":8,
    "num_workers":16,
    "if_test": false,
    "save_2d": false,
    "save_mode": "only_latest",
    "demo_freq": 100,

    "new_model": true,
    "render": false,

    "lambda_j2d_gt": 0.0001,
    "lambda_j3d": 100,
    "lambda_scale": 100,
    "lambda_pose": 0.1,


    "train_queries": ["trans_images","trans_Ks","trans_joints", "scales"],
    "val_queries": ["images","Ks"],
    "losses": ["joint_2d" , "joint_3d", "scale", "mpose"], 
    "train_requires": ["joints", "verts"],
    "test_requires": ["joints", "verts"],
    "task": "train",
    "mode": ["training"]
}