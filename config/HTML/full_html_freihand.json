{
    "train_datasets":["FreiHand"],
    "val_datasets":["FreiHand"],
    "freihand_base_path": "../freihand",
    "base_out_path": "outputs/freihand/fully_html",
    "is_write_tb": true,
    "writer_topic": "runs/fully_html/texture/",
    "total_epochs": 600,
    "init_lr": 0.001,
    "force_init_lr": 0.000001,
    "lr_steps": [80, 160, 200, 300, 350, 400, 450 ],
    "lr_gamma": 0.5,
    "save_interval": 10,
    "controlled_exp": false,
    "controlled_size": 100,
    "train_batch":16,
    "val_batch":16,
    "num_workers":8,
    "if_test": true,
    "save_2d": false,
    "save_3d": false,
    "save_mode": "only_latest",
    "demo_freq": 100,
    "only_train_regressor": true,
    "only_train_texture": true,

    "new_model": true,
    "render": true,
    "light_estimation": true,
    "four_channel": false,
    "hand_model": "nimble",
    "base_loss_fn": "L1",
    "use_mean_shape": false,
    "lambda_j3d": 200,
    "lambda_vert_3d": 150,
    "lambda_edge_len": 100,
    "lambda_silhouette": 0.0008,
    "lambda_tex_reg_steps": [260, 270],
    "lambda_tex_reg_list": [5e-3, 5e-5, 5e-7],
    "lambda_pose_steps": [10, 20],
    "lambda_pose_list": [0.01, 0.001, 0.00001],
    "lambda_shape_steps": [270, 300],
    "lambda_shape_list": [0.1, 0.001, 0.00001],
    "lambda_percep": 0.00000001,
    "lambda_mrgb": 2e-3,
    "lambda_ssim_tex": 0.01,
    "lambda_texture": 0.02,
    "lambda_bone_direc_3d": 6,


    "pretrain": "effb3",
    "pretrain_model": "outputs/freihand/fully_rhd/model/texturehand_520.t7",

    "train_queries": ["trans_images", "trans_Ks", "trans_joints", "scales", "trans_verts", "trans_masks"],
    "val_queries": [ "images", "Ks", "joints", "scales", "verts", "masks" ],
    "losses": [ "mtex", "texture", "mrgb", "sil", "ssim_tex", "perceptual" ],
    "train_requires": [ "joints", "verts" ],
    "test_requires": [ "joints", "verts" ],
    "task": "train",
    "mode": [ "training" ]
}